{
  "manifest_version": 3,
  "name": "LinkedIn Unfollow Everyone",
  "version": "1.0",
  "description": "Mass Unfollow everyone on LinkedIn",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ]
}
